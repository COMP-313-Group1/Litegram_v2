{
  "name": "litegram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "date-fns": "^2.29.3",
    "firebase": "^9.10.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
